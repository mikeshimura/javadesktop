{
  "name": "Spring React",
  "description": "Springboot sample use with Reactjs, dbflute",
  "keywords": [
    "springboot",
    "reactjs",
    "dbflute"
  ],
  "addons": [
    "heroku-postgresql:hobby-dev",
    "logentries:tryit",
    "pgbackups:auto-month"
  ],
  "env": {
    "JAVA_OPTS": "-Duser.language=ja -Duser.country=JP -Xmx384m -Xss512k -XX:+UseCompressedOops",
    "JAVA_TOOL_OPTIONS": "-Djava.net.preferIPv4Stack=true",
    "LANG": "ja_JP.UTF-8",
    "PATH": "/app/.jdk/bin:/usr/local/bin:/usr/bin:/bin",
    "TZ": "Asia/Tokyo"
  }
}